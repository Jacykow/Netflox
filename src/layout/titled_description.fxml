<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_CENTER" spacing="3.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="titleLabel" text="Title" textAlignment="CENTER" wrapText="true">
      <font>
         <Font name="System Bold Italic" size="20.0" />
      </font></Label>
    <Label fx:id="descriptionLabel" text="description" textAlignment="JUSTIFY" wrapText="true" />
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
